;; r6rs compatible config file

;; dir & prefix
;; change to konqueror's dir ~/.local/share/konqueror/ as you wish
(define dir "./example/")  
(define pre "bookmarks")

;; file name
(define bm-xml0 (string-append dir pre ".xml"))
(define bm-sxml0 (string-append dir pre "-sxml"))
(define bm-simp0 (string-append dir pre "-simp"))
(define bm-xml1 (string-append dir pre "1.xml"))
(define bm-sxml1 (string-append dir pre "-sxml1"))
(define bm-simp1 (string-append dir pre "-simp1"))
